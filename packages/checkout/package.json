{
  "name": "@f-git/checkout",
  "version": "1.0.2",
  "description": "git checkout",
  "type": "module",
  "keywords": [
    "git",
    "checkout",
    "cg"
  ],
  "author": "fafayzf <fafayzf@163.com>",
  "homepage": "https://github.com/fafayzf/cg/tree/main/packages/checkout#readme",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fafayzf/cg.git"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean": "rimraf node_modules && pnpm clean:dist"
  },
  "bugs": {
    "url": "https://github.com/fafayzf/cg/issues"
  },
  "dependencies": {
    "@f-git/init": "workspace:*",
    "@f-git/commit": "workspace:*",
    "@f-git/stash": "workspace:*",
    "color": "^4.2.3",
    "inquirer": "7.3.3",
    "simple-git": "^3.18.0"
  }
}
