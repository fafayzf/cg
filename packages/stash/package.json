  },
  "bugs": {
    "url": "https://github.com/fafayzf/f-git/issues"
  },
  "dependencies": {
    "@f-git/init": "workspace:*",
    "ora": "5.4.1",
    "simple-git": "^3.18.0"
  },
  "devDependencies": {
    "typescript": "^5.0.4"
  }
