{
  "name": "f-git",
  "version": "0.9.0",
  "description": "This is the git shortcut command package",
  "main": "./dist/node/index.js",
  "module": "./dist/node/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "import": "./dist/node/index.js",
      "require": "./dist/node-cjs/index.cjs"
    }
  },
  "files": [
    "dist",
    "types",
    "*.d.ts"
  ],
  "scripts": {
    "build": "pnpm run -r --parallel clean:dist && rollup -c",
    "example:init": "node ./examples/init.js",
    "example:pull": "node ./examples/pull.js",
    "example:push": "node ./examples/push.js",
    "example:checkout": "node ./examples/checkout.js",
    "pub": "sh build/release.sh",
    "clean": "rimraf node_modules && pnpm run -r --parallel clean",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fafayzf/cg.git"
  },
  "bin": {
    "cg": "./index.ts"
  },
  "keywords": [
    "git"
  ],
  "author": "fafayzf <fafayzf@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/color": "^3.0.3",
    "@types/inquirer": "^9.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.11",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.9",
    "esbuild-register": "^3.4.2",
    "fast-glob": "^3.2.12",
    "rollup": "^3.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "homepage": "https://github.com/fafayzf/f-git#readme",
  "bugs": {
    "url": "https://github.com/fafayzf/f-git/issues"
  },
  "dependencies": {
    "@f-git/checkout": "workspace:*",
    "@f-git/commit": "workspace:*",
    "@f-git/init": "workspace:*",
    "@f-git/pull": "workspace:*",
    "@f-git/push": "workspace:*",
    "@f-git/stash": "workspace:*",
    "commander": "^10.0.1",
    "simple-git": "^3.18.0"
  }
}
